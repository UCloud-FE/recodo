{
    "sourceType": "unambiguous",
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "ie": "11",
                    "firefox": "29",
                    "chrome": "30",
                    "safari": "7"
                },
                "spec": true
            }
        ],
        ["@babel/preset-react"],
        ["@babel/preset-typescript"]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "regenerator": false
            }
        ]
    ]
}
