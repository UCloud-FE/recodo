{
    "root": true,
    "extends": ["eslint:recommended", "plugin:node/recommended", "plugin:prettier/recommended"],
    "plugins": ["prettier", "node"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "node": true
    },
    "rules": {
        "no-console": "off"
    },
    "globals": {
        "ENV": "readonly"
    }
}
